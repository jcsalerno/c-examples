#include <stdio.h>
int main(){
    float peso, altura, imc;

    printf("Digite seu peso:");
    scanf ("%f", &peso);

    printf("Digite sua altura:");
    scanf ("%f", &altura);
    

    imc = peso / (altura * altura); 

    printf ("Seu peso é de %f, sua altura é %f e seuIMC é: %f", peso, altura, imc);

    return 0;
    
}

